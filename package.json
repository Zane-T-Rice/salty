{
  "name": "salty",
  "version": "1.0.0",
  "description": "A Discord Bot For My Personal Enjoyment",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "start": "node build/index.js",
    "lint": "eslint --fix --max-warnings=0 src/*.ts src/**/*.ts",
    "prettier": "prettier src/*.ts src/**/*.ts -w",
    "test": "rm -rf build && npm run build && jest build/ --coverage --verbose"
  },
  "author": "Zane Rice",
  "dependencies": {
    "async-mutex": "^0.5.0",
    "axios": "^1.7.0",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "git-hooks": "^1.1.10",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-2": "npm:prettier@^2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "prettierPath": "prettier-2"
  }
}
