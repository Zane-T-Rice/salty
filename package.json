{
  "name": "salty",
  "version": "1.0.0",
  "description": "A Discord Bot For My Personal Enjoyment",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "start": "nodemon build/index.js",
    "lint": "npx eslint --fix src/**/*.ts",
    "prettier": "npx prettier src/**/*.ts -w",
    "test": "rm -rf build && npx yarn build && jest build/ --coverage"
  },
  "author": "Zane Rice",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^13.0.0-dev",
    "dotenv": "^8.6.0",
    "typescript": "^4.3.5",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "git-hooks": "^1.1.10",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
